<div class="home-content">
  <div class="user-visual">
    <h2 class="page-title">users</h2>
  </div>
</div>

<div class="wrapper">
<% @users.each do |user| %>
  <div class="user-content">
  <%= image_tag user.get_profile_image, size: "50x50" %>
  <%= user.name %>
  <%= user.caption %>
  </div>
<% end %>
</div>
